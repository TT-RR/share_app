<div class="main users-new">
  <div class="container">
    <div class="form-heading">グループの代表者登録</div>
    <div class="form users-form">
      <div class="form-body">
        <!-- エラーメッセージを表示するためのHTMLを貼り付けてください -->
        <% @user.errors.full_messages.each do |message| %>
          <div class="form-error">
            <%= message %>
          </div>
        <% end %>
        
        <%= form_tag("/users/#{@user.id}/gcreate") do %>
          <p>グループID</p>
          
          <input name="gid_m" value="<%= @user.gid_m %>">
            
          <input type="submit" value="新規登録">
        <% end %>
      </div>
    </div>
  </div>
</div>